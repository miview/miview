{
  "name": "miview",
  "version": "0.2.19",
  "description": "`miview` 是一款基于 `Vue.js 2.0` 和 `ElementUI 2.0` 的前端 UI 通用组件库，主要用于快速开发 PC 网站中后台产品",
  "homepage": "https://ly2011.github.io/miview",
  "main": "lib/index.js",
  "module": "es/index.js",
  "style": "lib/index.css",
  "typings": "types/index.d.ts",
  "files": [
    "es",
    "lib",
    "packages",
    "types",
    "vetur"
  ],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git@github.com:ly2011/miview.git"
  },
  "keywords": [
    "miview",
    "vue",
    "component"
  ],
  "author": "ly2011",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/miview/miview/issues"
  },
  "scripts": {
    "bootstrap": "yarn || npm i",
    "dev": "npm-run-all --parallel build:entry dev:site",
    "dev:example": "webpack-dev-server --config build/webpack.dev.js",
    "dev:site": "webpack-dev-server --config build/webpack.doc.js",
    "lint": "eslint ./packages --ext .js,.vue && stylelint \"packages/*.css\"",
    "build:entry": "node build/build-entry.js",
    "build:components": "node build/build-components.js --color",
    "build:packages": "cross-env NODE_ENV=production webpack --color --config build/webpack.build.js && cross-env NODE_ENV=production webpack -p --color --config build/webpack.build.js",
    "build:style": "gulp --gulpfile ./build/build-style.js",
    "build:style-entry": "node build/build-style-entry.js",
    "build:changelog": "vant-doc changelog ./changelog.generated.md",
    "build:lib": "node build/build-lib.js",
    "build:site": "rimraf docs/dist && cross-env NODE_ENV=production webpack --config build/webpack.doc.js && gh-pages -d docs/dist",
    "release": "sh build/release.sh"
  },
  "dependencies": {
    "@babel/runtime": "^7.3.0",
    "area-data": "^5.0.6",
    "element-ui": "^2.4.11"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-jsx": "^7.2.0",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.1.6",
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "@vant/compiler": "^1.0.0",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.0.0-beta.2",
    "@vue/babel-preset-jsx": "^1.0.0-beta.2",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-component": "^1.1.1",
    "commitizen": "^3.0.4",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.1",
    "cssnano": "^4.1.7",
    "cz-conventional-changelog": "^2.1.0",
    "dependency-tree": "^7.0.2",
    "eslint": "^5.8.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "fast-glob": "^2.2.6",
    "file-loader": "^2.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "fs-extra": "^7.0.1",
    "gh-pages": "^2.0.1",
    "gulp": "3.9.1",
    "gulp-csso": "^3.0.1",
    "gulp-postcss": "^8.0.0",
    "gulp-sass": "^4.0.2",
    "highlight.js": "^9.13.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.1.4",
    "lerna": "^3.4.3",
    "lint-staged": "^7.2.2",
    "markdown-it-anchor": "^5.0.2",
    "markdown-it-container": "^2.0.0",
    "markdown-vetur": "^0.0.5",
    "mini-css-extract-plugin": "^0.5.0",
    "moment": "^2.22.2",
    "node-sass": "^4.10.0",
    "npm-run-all": "^4.1.5",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.4.0",
    "postcss-pxtorem": "^4.0.1",
    "prettier": "^1.15.2",
    "progress-bar-webpack-plugin": "^1.11.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "shelljs": "^0.8.3",
    "signale": "^1.3.0",
    "stylelint": "^9.10.1",
    "uppercamelcase": "^3.0.0",
    "url-loader": "^1.1.2",
    "vue-loader": "^15.4.2",
    "vue-markdown-loader": "^2.4.1",
    "vue-router": "^3.0.2",
    "vue-template-compiler": "^2.5.17",
    "vue-template-es2015-compiler": "^1.8.1",
    "vuedraggable": "^2.16.0",
    "vuex": "^3.1.0",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.4"
  },
  "peerDependencies": {
    "vue": ">= 2.5.0",
    "element-ui": ">= 2.3.9"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "packages/**/*.{js,vue}": [
      "eslint",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
